root = true

[*]
charset = utf-8
end_of_line = crlf
indent_size = 4
indent_style = space
insert_final_newline = false
max_line_length = 340
tab_width = 4

[*.dart]
max_line_length = 200

[{*.apinotes,*.yaml,*.yml,.clang-format,.clang-tidy,_clang-format,pubspec.lock}]
indent_size = 2

[{*.bash,*.sh,*.zsh}]
indent_size = 2
tab_width = 2

[{*.har,*.json,*.jsonc}]
indent_size = 2
